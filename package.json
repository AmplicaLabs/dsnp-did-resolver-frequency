{
  "name": "@amplica-labs/dsnp-did-resolver-plugin-frequency",
  "version": "0.0.0",
  "description": "A DSNP over Frequency DID resolver",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "prepare": "semver -r '>=9.0.0' $npm_config_npm_version && cd node_modules/@dsnp/did-resolver && npm run prepack",
    "check-env": "node -e 'console.log(process.env)' | grep npm",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "format": "tsc --noEmit --pretty && eslint --fix \"**/*.ts\"",
    "lint": "tsc --noEmit --pretty && eslint \"**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AmplicaLabs/dsnp-did-resolver-plugin-frequency.git"
  },
  "author": "Amplica Labs, LLC",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/AmplicaLabs/dsnp-did-resolver-plugin-frequency/issues"
  },
  "homepage": "https://github.com/AmplicaLabs/dsnp-did-resolver-plugin-frequency#readme",
  "dependencies": {
    "@dsnp/did-resolver": "github:LibertyDSNP/dsnp-did-resolver",
    "@dsnp/frequency-schemas": "file:../schemas/dist",
    "@frequency-chain/api-augment": "^1.8.1",
    "@polkadot/api": "^10.9.1",
    "avro-js": "^1.11.3",
    "did-resolver": "^4.1.0",
    "dotenv": "^16.3.1",
    "multiformats": "^12.1.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
